  "verses": [
    {
      "global_id": 6059,
      "surah": 92,
      "ayah": 1,
      "verse_key": "92:1",
      "text_ar": " وَٱلَّيْلِ إِذَا يَغْشَىٰ"
    },
    {
      "global_id": 6060,
      "surah": 92,
      "ayah": 2,
      "verse_key": "92:2",
      "text_ar": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ"
    },
    {
      "global_id": 6061,
      "surah": 92,
      "ayah": 3,
      "verse_key": "92:3",
      "text_ar": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ"
    },
    {
      "global_id": 6062,
      "surah": 92,
      "ayah": 4,
      "verse_key": "92:4",
      "text_ar": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ"
    },
    {
      "global_id": 6063,
      "surah": 92,
      "ayah": 5,
      "verse_key": "92:5",
      "text_ar": "فَأَمَّا مَنْ أَعْطَىٰ وَٱتَّقَىٰ"
    },
    {
      "global_id": 6064,
      "surah": 92,
      "ayah": 6,
      "verse_key": "92:6",
      "text_ar": "وَصَدَّقَ بِٱلْحُسْنَىٰ"
    },
    {
      "global_id": 6065,
      "surah": 92,
      "ayah": 7,
      "verse_key": "92:7",
      "text_ar": "فَسَنُيَسِّرُهُۥ لِلْيُسْرَىٰ"
    },
    {
      "global_id": 6066,
      "surah": 92,
      "ayah": 8,
      "verse_key": "92:8",
      "text_ar": "وَأَمَّا مَنۢ بَخِلَ وَٱسْتَغْنَىٰ"
    },
    {
      "global_id": 6067,
      "surah": 92,
      "ayah": 9,
      "verse_key": "92:9",
      "text_ar": "وَكَذَّبَ بِٱلْحُسْنَىٰ"
    },
    {
      "global_id": 6068,
      "surah": 92,
      "ayah": 10,
      "verse_key": "92:10",
      "text_ar": "فَسَنُيَسِّرُهُۥ لِلْعُسْرَىٰ"
    },
    {
      "global_id": 6069,
      "surah": 92,
      "ayah": 11,
      "verse_key": "92:11",
      "text_ar": "وَمَا يُغْنِى عَنْهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ"
    },
    {
      "global_id": 6070,
      "surah": 92,
      "ayah": 12,
      "verse_key": "92:12",
      "text_ar": "إِنَّ عَلَيْنَا لَلْهُدَىٰ"
    },
    {
      "global_id": 6071,
      "surah": 92,
      "ayah": 13,
      "verse_key": "92:13",
      "text_ar": "وَإِنَّ لَنَا لَلْـَٔاخِرَةَ وَٱلْأُولَىٰ"
    },
    {
      "global_id": 6072,
      "surah": 92,
      "ayah": 14,
      "verse_key": "92:14",
      "text_ar": "فَأَنذَرْتُكُمْ نَارًا تَلَظَّىٰ"
    },
    {
      "global_id": 6073,
      "surah": 92,
      "ayah": 15,
      "verse_key": "92:15",
      "text_ar": "لَا يَصْلَىٰهَآ إِلَّا ٱلْأَشْقَى"
    },
    {
      "global_id": 6074,
      "surah": 92,
      "ayah": 16,
      "verse_key": "92:16",
      "text_ar": "ٱلَّذِى كَذَّبَ وَتَوَلَّىٰ"
    },
    {
      "global_id": 6075,
      "surah": 92,
      "ayah": 17,
      "verse_key": "92:17",
      "text_ar": "وَسَيُجَنَّبُهَا ٱلْأَتْقَى"
    },
    {
      "global_id": 6076,
      "surah": 92,
      "ayah": 18,
      "verse_key": "92:18",
      "text_ar": "ٱلَّذِى يُؤْتِى مَالَهُۥ يَتَزَكَّىٰ"
    },
    {
      "global_id": 6077,
      "surah": 92,
      "ayah": 19,
      "verse_key": "92:19",
      "text_ar": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعْمَةٍ تُجْزَىٰٓ"
    },
    {
      "global_id": 6078,
      "surah": 92,
      "ayah": 20,
      "verse_key": "92:20",
      "text_ar": "إِلَّا ٱبْتِغَآءَ وَجْهِ رَبِّهِ ٱلْأَعْلَىٰ"
    },
    {
      "global_id": 6079,
      "surah": 92,
      "ayah": 21,
      "verse_key": "92:21",
      "text_ar": "وَلَسَوْفَ يَرْضَىٰ"
    }
  ]
