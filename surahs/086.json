      "text_ar": " وَٱلسَّمَآءِ وَٱلطَّارِقِ"
    },
    {
      "global_id": 5933,
      "surah": 86,
      "ayah": 2,
      "verse_key": "86:2",
      "text_ar": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ"
    },
    {
      "global_id": 5934,
      "surah": 86,
      "ayah": 3,
      "verse_key": "86:3",
      "text_ar": "ٱلنَّجْمُ ٱلثَّاقِبُ"
    },
    {
      "global_id": 5935,
      "surah": 86,
      "ayah": 4,
      "verse_key": "86:4",
      "text_ar": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ"
    },
    {
      "global_id": 5936,
      "surah": 86,
      "ayah": 5,
      "verse_key": "86:5",
      "text_ar": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ مِمَّ خُلِقَ"
    },
    {
      "global_id": 5937,
      "surah": 86,
      "ayah": 6,
      "verse_key": "86:6",
      "text_ar": "خُلِقَ مِن مَّآءٍ دَافِقٍ"
    },
    {
      "global_id": 5938,
      "surah": 86,
      "ayah": 7,
      "verse_key": "86:7",
      "text_ar": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ"
    },
    {
      "global_id": 5939,
      "surah": 86,
      "ayah": 8,
      "verse_key": "86:8",
      "text_ar": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌ"
    },
    {
      "global_id": 5940,
      "surah": 86,
      "ayah": 9,
      "verse_key": "86:9",
      "text_ar": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ"
    },
    {
      "global_id": 5941,
      "surah": 86,
      "ayah": 10,
      "verse_key": "86:10",
      "text_ar": "فَمَا لَهُۥ مِن قُوَّةٍ وَلَا نَاصِرٍ"
    },
    {
      "global_id": 5942,
      "surah": 86,
      "ayah": 11,
      "verse_key": "86:11",
      "text_ar": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ"
    },
    {
      "global_id": 5943,
      "surah": 86,
      "ayah": 12,
      "verse_key": "86:12",
      "text_ar": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ"
    },
    {
      "global_id": 5944,
      "surah": 86,
      "ayah": 13,
      "verse_key": "86:13",
      "text_ar": "إِنَّهُۥ لَقَوْلٌ فَصْلٌ"
    },
    {
      "global_id": 5945,
      "surah": 86,
      "ayah": 14,
      "verse_key": "86:14",
      "text_ar": "وَمَا هُوَ بِٱلْهَزْلِ"
    },
    {
      "global_id": 5946,
      "surah": 86,
      "ayah": 15,
      "verse_key": "86:15",
      "text_ar": "إِنَّهُمْ يَكِيدُونَ كَيْدًا"
    },
    {
      "global_id": 5947,
      "surah": 86,
      "ayah": 16,
      "verse_key": "86:16",
      "text_ar": "وَأَكِيدُ كَيْدًا"
    },
    {
      "global_id": 5948,
      "surah": 86,
      "ayah": 17,
      "verse_key": "86:17",
      "text_ar": "فَمَهِّلِ ٱلْكَـٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا"
    }
  ]
