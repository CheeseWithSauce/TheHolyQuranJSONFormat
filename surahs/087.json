  "verses": [
    {
      "global_id": 5949,
      "surah": 87,
      "ayah": 1,
      "verse_key": "87:1",
      "text_ar": " سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى"
    },
    {
      "global_id": 5950,
      "surah": 87,
      "ayah": 2,
      "verse_key": "87:2",
      "text_ar": "ٱلَّذِى خَلَقَ فَسَوَّىٰ"
    },
    {
      "global_id": 5951,
      "surah": 87,
      "ayah": 3,
      "verse_key": "87:3",
      "text_ar": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ"
    },
    {
      "global_id": 5952,
      "surah": 87,
      "ayah": 4,
      "verse_key": "87:4",
      "text_ar": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ"
    },
    {
      "global_id": 5953,
      "surah": 87,
      "ayah": 5,
      "verse_key": "87:5",
      "text_ar": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ"
    },
    {
      "global_id": 5954,
      "surah": 87,
      "ayah": 6,
      "verse_key": "87:6",
      "text_ar": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ"
    },
    {
      "global_id": 5955,
      "surah": 87,
      "ayah": 7,
      "verse_key": "87:7",
      "text_ar": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ"
    },
    {
      "global_id": 5956,
      "surah": 87,
      "ayah": 8,
      "verse_key": "87:8",
      "text_ar": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ"
    },
    {
      "global_id": 5957,
      "surah": 87,
      "ayah": 9,
      "verse_key": "87:9",
      "text_ar": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ"
    },
    {
      "global_id": 5958,
      "surah": 87,
      "ayah": 10,
      "verse_key": "87:10",
      "text_ar": "سَيَذَّكَّرُ مَن يَخْشَىٰ"
    },
    {
      "global_id": 5959,
      "surah": 87,
      "ayah": 11,
      "verse_key": "87:11",
      "text_ar": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى"
    },
    {
      "global_id": 5960,
      "surah": 87,
      "ayah": 12,
      "verse_key": "87:12",
      "text_ar": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ"
    },
    {
      "global_id": 5961,
      "surah": 87,
      "ayah": 13,
      "verse_key": "87:13",
      "text_ar": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ"
    },
    {
      "global_id": 5962,
      "surah": 87,
      "ayah": 14,
      "verse_key": "87:14",
      "text_ar": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ"
    },
    {
      "global_id": 5963,
      "surah": 87,
      "ayah": 15,
      "verse_key": "87:15",
      "text_ar": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ"
    },
    {
      "global_id": 5964,
      "surah": 87,
      "ayah": 16,
      "verse_key": "87:16",
      "text_ar": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا"
    },
    {
      "global_id": 5965,
      "surah": 87,
      "ayah": 17,
      "verse_key": "87:17",
      "text_ar": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ"
    },
    {
      "global_id": 5966,
      "surah": 87,
      "ayah": 18,
      "verse_key": "87:18",
      "text_ar": "إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ"
    },
    {
      "global_id": 5967,
      "surah": 87,
      "ayah": 19,
      "verse_key": "87:19",
      "text_ar": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ"
    }
  ]
}